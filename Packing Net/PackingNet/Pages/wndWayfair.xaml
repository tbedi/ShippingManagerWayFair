                <TextBlock Canvas.Left="10" TextWrapping="Wrap" Text="QTY:" Canvas.Top="131" Width="31" Style="{StaticResource Sliptext}"/>
                <TextBlock Canvas.Left="10" TextWrapping="Wrap" Text="Part#:" Canvas.Top="154" RenderTransformOrigin="0.6,2.5" Style="{StaticResource Sliptext}"/>
                <TextBlock Canvas.Left="9" TextWrapping="Wrap" Text="UPC :" Canvas.Top="179" Style="{StaticResource Sliptext}"/>
                <TextBlock Canvas.Left="205" TextWrapping="Wrap" Text="Color :" Canvas.Top="131" Style="{StaticResource Sliptext}"/>
                <TextBlock  Name="txtWH" Canvas.Left="100" TextWrapping="Wrap" Text="Null" Canvas.Top="112" Style="{StaticResource Sliptext}"/>
                <TextBlock Name="txtQty" Canvas.Left="49" TextWrapping="Wrap" Text="Null" Canvas.Top="131" Width="55" Style="{StaticResource Sliptext}"/>
                <TextBlock Name="txtPartNumber" Canvas.Left="49" TextWrapping="Wrap" Text="Null" Canvas.Top="155" Width="125" Style="{StaticResource Sliptext}"/>
                <TextBlock Canvas.Left="49" TextWrapping="Wrap" Text="Null" Canvas.Top="179" Width="149" Style="{StaticResource Sliptext}" x:Name="txtupc"/>
                <TextBlock Canvas.Left="250" TextWrapping="Wrap" Text="Brown" Canvas.Top="131" RenderTransformOrigin="-0.318,5.188" Width="61" Style="{StaticResource Sliptext}"/>
                <TextBlock Canvas.Left="197" TextWrapping="Wrap" Text="Carton :" Canvas.Top="154" Style="{StaticResource Sliptext}"/>
                <TextBlock Name="txtCarton" Canvas.Left="250" TextWrapping="Wrap" Text="Null" Canvas.Top="155" RenderTransformOrigin="5.136,3.625" Width="54" Style="{StaticResource Sliptext}"/>
                <TextBlock Canvas.Left="204" TextWrapping="Wrap" Text="Box " Canvas.Top="291" Style="{StaticResource Sliptext}"/>
                <TextBlock Name="txtBoxNumber" Canvas.Left="250" TextWrapping="Wrap" Text="Null" Canvas.Top="291" RenderTransformOrigin="-0.318,5.188" Width="61" Style="{StaticResource Sliptext}"/>
                <Image Height="86" Canvas.Left="27" Canvas.Top="200" Width="270" Name="image"/>
                <TextBlock Canvas.Left="99" TextWrapping="Wrap" Text="PO # :" Canvas.Top="85" Style="{StaticResource Sliptext}"/>

            </Canvas>
        </Border>
    </Grid>
</Window>
